      "source": [
        "def solution(array, commands):\n",
        "    answer = []\n",
        "    for command in commands:\n",
        "        s = command[0]-1\n",
        "        e = command[1]\n",
        "        n = command[2]-1\n",
        "        temp = array[s:e]\n",
        "        temp.sort()\n",
        "        answer.append(temp[n])\n",
        "    return answer"
      ],
